            MENUITEM "&VCenter of Left edge",       ID_ANCHOR_LEFTEDGE_VCENTER

            MENUITEM "&Bottom of Left edge",        ID_ANCHOR_LEFTEDGE_BOTTOM

            MENUITEM SEPARATOR
            MENUITEM "&Top of Right edge",          ID_ANCHOR_RIGHTEDGE_TOP
            MENUITEM "&VCenter of Right edge",      ID_ANCHOR_RIGHTEDGE_VCENTER

            MENUITEM "&Bottom of Right edge",       ID_ANCHOR_RIGHTEDGE_BOTTOM


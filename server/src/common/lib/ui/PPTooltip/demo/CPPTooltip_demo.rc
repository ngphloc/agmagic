    END
    POPUP "&Help"
    BEGIN
        MENUITEM "&About CPPTooltip_demo...",   ID_APP_ABOUT
    END
    POPUP "&Edit"
    BEGIN
        MENUITEM "&Copy",                       ID_EDIT_COPY
        MENUITEM "Cu&t",                        ID_EDIT_CUT
        MENUITEM "&Paste",                      ID_EDIT_PASTE, GRAYED
    END
END


